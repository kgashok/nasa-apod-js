<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Example APOD call</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link id="favicon" rel="icon" href="https://www.nasa.gov/favicon.ico" type="image/vnd.microsoft.icon"">

    <!-- import the webpage's stylesheet -->
    <link rel="stylesheet" href="/style.css">
    <!-- import the webpage's javascript file -->
    <script src="/script.js" defer></script>
  </head>  
  <body>
    <main>
      <b>API URL:</b>
      <pre id="reqObject"></pre>
      <img id="apod_img_id" width="250px"/>
      <iframe id="apod_vid_id" type="text/html" width="640" height="385" frameborder="0"></iframe>
      <p id="copyright"></p>
      <h3 id="apod_title"></h3>
      <p id="apod_explanation"></p>
      <br/>
      <b>Return Object:</b>
      <pre id="returnObject"></pre>
      <h1>Hi there!</h1>
      <figure> 
        <img alt="The King"
             class= "me" 
             src="https://www.nationalgeographic.com/content/dam/photography/rights-exempt/best-of-photo-of-the-day/2018/june/03_best-pod-june-18.adapt.945.1.jpg">
        <figcaption>Me, The King, lounging away...</figcaption>
      </figure>
      <h2>
        Photo of the Day
      </h2>
      <em>Credits: Unsplash</em>
      <br>
      <img class= "daily" src="https://source.unsplash.com/weekly?digital">
    </main>
    <footer>
      I'm your cool new webpage. Made with <a href="https://glitch.com">Glitch</a>!
    </footer>
    <!-- include the Glitch button to show what the webpage is about and
to make it easier for folks to view source and remix -->
    <div class="glitchButton" style="position:fixed;top:20px;right:20px;"></div>
    <script src="https://button.glitch.me/button.js"></script>
  </body>
</html>
